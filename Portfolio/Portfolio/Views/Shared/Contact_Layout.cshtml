@{
    ViewBag.Title = "Contact_Layout";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    
</head>
<body>
    <header>
        <div class="ContactHeader">
        <div class="LogoImage">
            <img src="images.logo_1032481_web.jpg)" height="200px" width="200px"/>
        </div>
        
        <div class="ContactHeadRight">
        <h3 class="Head">
            <a href="@Url.Action("Index", "Home")">
                Home 
            </a>
            &nbsp;|&nbsp;
            <a href="@Url.Action("Index","About")">
               About 
            </a>
            &nbsp;|&nbsp;    
            <a href="@Url.Action("Index","Work")">
                Work
            </a>    
            &nbsp;|&nbsp;
            <a href="@Url.Action("Index","Contact")">
                Contact
            </a>     
            </h3>
            </div>
            </div>
    </header>

    @RenderBody()


    <footer class="Footer">
        <div class="footerLeft"> Copyright (c) 2014 Jaya Chunduri </div>
        <div class="footerRight"> 
            <a href="@Url.Action("Index", "Home")">
                Home 
            </a>
            &nbsp;|&nbsp;
            <a href="@Url.Action("Index","About")">
               About 
            </a>
            &nbsp;|&nbsp;    
            <a href="@Url.Action("Index","Work")">
                Work
            </a>    
            &nbsp;|&nbsp;
            <a href="@Url.Action("Index","Contact")">
                Contact
            </a>     
       </div>
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>

